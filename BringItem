-- Spawn Items near you.
for i,v in pairs(game:GetService("Workspace").WeaponDrops:GetDescendants()) do
    if v.ClassName == "MeshPart" or v.ClassName == "Part" then
        v.CFrame = CFrame.new(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position + Vector3.new(math.random(-5, 5), 0, math.random(-10, 10)))
    end
end

-- Spawn other things near you
for i,v in pairs(game:GetService("Workspace").Interactables:GetDescendants()) do
    if v.Name == "Interact" or v.ClassName == "MeshPart"  then
        v.CFrame = CFrame.new(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position + Vector3.new(math.random(-5, 10), 0, math.random(-10, 10)))
    end
end

-- Destroy Obstacles
for i,v in pairs(game:GetService("Workspace").Interactables:GetChildren()) do
    if v.Name == "Double Locker" or v.Name == "Locker" or v.Name == "LockerGroup" or v.Name == "Barrel" or v.Name == "ScavTraps" or v.Name == "Medical Cabinet" or v.Name == "Crate" then
        v:Destroy()
        wait()
    end
end
